<section class="sub-nav">
    <section class="sub-nav-left">
        <h3>Register</h3>
    </section>
</section>

<form action="/auth/register" method="POST">
    <p>EMAIL</p>
    <input id="authEmail" class="black" type="email" name="email">
    <p>USERNAME</p>
    <input id="authName" class="black" type="text" name="name">
    <p>PASSWORD</p>
    <input id="authPassword" class="black" type="password" name="password">
    <input class="btn" type="submit">
    <%- include('../partials/alerts') %>
</form>